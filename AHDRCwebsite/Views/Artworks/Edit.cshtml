<h1 class="text-light">Edit</h1>

<div>
    <a asp-action="Index" class="text-light">Back to archive</a>
</div>

<div class="text-light row info-div justify-content-center">
    <div class="col-5 image-col">
        @if (Model.ArtworkImage != null && Model.ArtworkImage.Count() >= 1)
        {
            <div id="custCarousel" class="carousel slide" data-interval="false" align="center">
                <div class="carousel-inner">
                    @foreach (var item in Model.ArtworkImage)
                    {
                        var totalAmount = Model.ArtworkImage.Count();
                        var itemList = Model.ArtworkImage.ToList();
                        @if (Model.ArtworkImage.First() == item)
                        {
                            <div class="carousel-item active">
                                <a asp-controller="ArtworkImages" asp-action="delete" asp-route-artworkimageid="@item.ArtworkImageId" data-artworkimageid="@item.ArtworkImageId"><img id="info-image" src=@("/uploads/" + @item.ImageURL) class="img-fluid"></a>
                            </div>
                        }
                        else
                        {
                            <div class="carousel-item">
                                <a asp-controller="ArtworkImages" asp-action="delete" asp-route-artworkimageid="@item.ArtworkImageId" data-artworkimageid="@item.ArtworkImageId"><img id="info-image" src=@("/uploads/" + @item.ImageURL) class="img-fluid"></a>
                            </div>
                        }
                        @if (Model.ArtworkImage.Last() == item)
                        {
                            <ol class="carousel-indicators list-inline" id="carousel-thumbnails">
                                @for (int i = 0; i < totalAmount; i++)
                                {
                                    var itemThumbnail = itemList[i];
                                    @if (i == 0)
                                    {
                                        <li class="list-inline-item">
                                            <a id="@("carousel-selector-" + i)" class="selected" data-slide-to="@(i)" data-target="#custCarousel" data-artworkimageid="@item.ArtworkImageId">
                                                <img src=@("/uploads/" + @itemThumbnail.ImageURL)>
                                            </a>
                                        </li>
                                    }
                                    else
                                    {
                                        <li>
                                            <a id="@("carousel-selector-" + i)" data-slide-to="@(i)" data-target="#custCarousel" data-artworkimageid="@item.ArtworkImageId">
                                                <img src=@("/uploads/" + @itemThumbnail.ImageURL)>
                                            </a>
                                        </li>
                                    }
                                }
                            </ol>
                        }
                    }
                </div>
            </div>

        }
        else
        {
            <div id="custCarousel" class="carousel slide" data-interval="false" align="center">
                <div class="carousel-inner">
                    <div class="carousel-item active">
                        <a href="@("/Images/defaultimage")"><img src=@("/Images/defaultimage.jpg") class="img-fluid"></a>
                    </div>
                </div>
            </div>
        }
    </div>

    <div class="col-auto button-group">
        @if (User.Identity.IsAuthenticated)
        {
            <img src=@("/images/download.png")>
            <img src=@("/images/comment.png")>
        }
        @if (User.IsInRole("Administrator"))
        {

            <a asp-controller="ArtworkImages" asp-action="Create" asp-route-artworkid="@Model.ArtworkId"><img src=@("/images/pic_add.png")></a>
            if (Model.ArtworkImage.Count > 0)
            {


                var test = Model.ArtworkImage.ToList().FirstOrDefault().ArtworkImageId;
                <a asp-controller="ArtworkImages" asp-action="Delete" asp-route-artworkimageid="@test" id="deleteLink"><img src=@("/images/pic_delete.png")></a>
            }
            <a asp-action="Edit" asp-route-artworkid="@Model.ArtworkId"><img src=@("/images/page_edit.png")></a>
            <a asp-action="Create"><img src=@("/images/page_new.png")></a>
