@model AHDRCwebsite.Models.Artwork

@{
    ViewData["Title"] = "Details";
}

<link rel="stylesheet" href="~/css/cardoverlay.css">
<style>
    .info-div {
        width: 100vw;
        position: relative;
        left: calc(-50vw + 50%);
    }

        .info-div img {
            width: 100%;
        }

        tbody tr td:first-child {
  width: 8em;
  min-width: 8em;
  max-width: 8em;
  word-break: break-all;
}

    .button-group {
        display: flex;
        flex-direction: column;
        padding: 0 0 0 0;
    }

        .button-group img {
            width: 60px;
            height: 60px;
            margin-bottom: 10px;
        }

    #custCarousel .carousel-indicators {
        position: relative;
        margin-top: 20px;
        display: flex;
        flex-direction: column;
        margin: 0 0 0 0;
        right: 0;
    }

        #custCarousel .carousel-indicators > li {
            border-bottom: 1px solid transparent;
            border-top: 1px solid transparent;
            width: 40px;
            height: 40px;
            opacity: 1;
        }

        #custCarousel .carousel-indicators li img {
            height: 100%;
            width: 100%;
            display: block;
            z-index: 100;
            border: 2px solid grey;
        }



    h1 {
        font-size: 20px;
        overflow: hidden;
        text-align: center;
    }

        h1:before,
        h1:after {
            background-color: white;
            content: "";
            display: inline-block;
            height: 1px;
            position: relative;
            vertical-align: middle;
            width: 50%;
        }

        h1:before {
            right: 0.5em;
            margin-left: -50%;
        }

        h1:after {
            left: 0.5em;
            margin-right: -50%;
        }
        td { vertical-align:top;
        }
        
</style>
<script>
    function hideObjectInfo() {
        var divName = "objectInfo";
        hideDiv(divName);
    }

    function hidePhysicalInfo() {
        var divName = "physicalInfo";
        hideDiv(divName);
    }
    function hideGeographicalInfo() {
        var divName = "geographicalInfo";
        hideDiv(divName);
    }
    function hideAuthorshipInfo() {
        var divName = "authorshipInfo";
        hideDiv(divName);
    }
    function hideHistoryInfo() {
        var divName = "historyInfo";
        hideDiv(divName);
    }
    function hideNotesInfo() {
        var divName = "notesInfo";
        hideDiv(divName);
    }
    function hideClassificationInfo() {
        var divName = "classificationInfo";
        hideDiv(divName);
    }
    function hideAuthorityInfo() {
        var divName = "authorityInfo";
        hideDiv(divName);
    }

    function hideDiv(divName) {
        var x = document.getElementById(divName);
        if (x.style.display === "none") {
            x.style.display = "block";
        } else {
            x.style.display = "none";
        }
    }


</script>
<div>
    <a a asp-action="Index" asp-route-currentFilter="@ViewData["CurrentFilter"]"
   asp-route-currentCategory="@ViewData["CurrentSelectedCategory"]" asp-route-pageNumber="@ViewData["pageNumber"]">Back to search results</a>
</div>
<div class="text-light row info-div justify-content-center">
    <div class="col-5 image-col">
        @if (Model.ArtworkImage.Count >= 1)
        {
            <div id="custCarousel" class="carousel slide" data-interval="false" align="center">
                <div class="carousel-inner">
                    @foreach (var item in Model.ArtworkImage)
                    {
                        var totalAmount = Model.ArtworkImage.Count();
                        var itemList = Model.ArtworkImage.ToList();
                        @if (Model.ArtworkImage.First() == item)
                        {
                            <div class="carousel-item active">
                                <a href="@("/uploads/" + @item.ImageURL)"><img src=@("/uploads/" + @item.ImageURL) class="img-fluid"></a>
                            </div>
                        }
                        else
                        {
                            <div class="carousel-item">
                                <a href="@("/uploads/" + @item.ImageURL)"><img src=@("/uploads/" + @item.ImageURL) class="img-fluid"></a>
                            </div>
                        }

                        @if (Model.ArtworkImage.Last() == item)
                        {
                            <ol class="carousel-indicators list-inline">
                                @for (int i = 0; i < totalAmount; i++)
                                {
                                    var itemThumbnail = itemList[i];
                                    @if (i == 0)
                                    {

                                        <li class="list-inline-item">
                                            <a id="@("carousel-selector-" + i)" class="selected" data-slide-to="@(i)" data-target="#custCarousel">
                                                <img src=@("/uploads/" + @itemThumbnail.ImageURL)>
                                            </a>
                                        </li>
                                    }
                                    else
                                    {
                                        <li>
                                            <a id="@("carousel-selector-" + i)" data-slide-to="@(i)" data-target="#custCarousel">
                                                <img src=@("/uploads/" + @itemThumbnail.ImageURL)>
                                            </a>
                                        </li>
                                    }
                                }
                            </ol>
                        }
                    }
                </div>
            </div>

        } else
        {
            <div id="custCarousel" class="carousel slide" data-interval="false" align="center">
                <div class="carousel-inner">
                            <div class="carousel-item active">
                                <a href="@("/Images/defaultimage")"><img src=@("/Images/defaultimage.jpg") class="img-fluid"></a>
                            </div>
                </div>
            </div>
        }
    </div>

    <div class="col-auto button-group">
        @if(User.Identity.IsAuthenticated)
        {
        <img src=@("/images/download.png")>
        <img src=@("/images/comment.png")>
        <a asp-controller="ArtworkImages" asp-action="Create" asp-route-id="@Model.Id"><img src=@("/images/pic_add.png")></a>
        }
        @if(User.IsInRole("Administrator"))
        {
        <a asp-action="Edit" asp-route-id="@Model.Id"><img src=@("/images/page_edit.png")></a>
        <a asp-action="Create"><img src=@("/images/page_new.png")></a>
        <a asp-action="Delete" asp-route-id="@Model.Id"><img src=@("/images/page_delete.png")></a>
        <img src=@("/images/page_move.png")>
        <img src=@("/images/book.png")>
        //we are still missing some buttons!
        }
