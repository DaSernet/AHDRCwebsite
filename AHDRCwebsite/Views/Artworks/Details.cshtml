    <div class="col-5 image-col">
        @if (Model.ArtworkImage.Count >= 1)
        {
            <div id="custCarousel" class="carousel slide" data-interval="false" align="center">
                <div class="carousel-inner">
                    @foreach (var item in Model.ArtworkImage)
                    {
                        var totalAmount = Model.ArtworkImage.Count();
                        var itemList = Model.ArtworkImage.ToList();
                        @if (Model.ArtworkImage.First() == item)
                        {
                            <div class="carousel-item active">
                                <a href="@("/uploads/" + @item.ImageURL)"><img src=@("/uploads/" + @item.ImageURL) class="img-fluid"></a>
                            </div>
                        }
                        else
                        {
                            <div class="carousel-item">
                                <a href="@("/uploads/" + @item.ImageURL)"><img src=@("/uploads/" + @item.ImageURL) class="img-fluid"></a>
                            </div>
                        }

                        @if (Model.ArtworkImage.Last() == item)
                        {
                            <ol class="carousel-indicators list-inline">
                                @for (int i = 0; i < totalAmount; i++)
                                {
                                    var itemThumbnail = itemList[i];
                                    @if (i == 0)
                                    {

                                        <li class="list-inline-item">
                                            <a id="@("carousel-selector-" + i)" class="selected" data-slide-to="@(i)" data-target="#custCarousel">
                                                <img src=@("/uploads/" + @itemThumbnail.ImageURL)>
                                            </a>
                                        </li>
                                    }
                                    else
                                    {
                                        <li>
                                            <a id="@("carousel-selector-" + i)" data-slide-to="@(i)" data-target="#custCarousel">
                                                <img src=@("/uploads/" + @itemThumbnail.ImageURL)>
                                            </a>
                                        </li>
                                    }
                                }
                            </ol>
                        }
                    }
                </div>
            </div>

        } else
        {
            <div id="custCarousel" class="carousel slide" data-interval="false" align="center">
                <div class="carousel-inner">
                            <div class="carousel-item active">
                                <a href="@("/Images/defaultimage")"><img src=@("/Images/defaultimage.jpg") class="img-fluid"></a>
                            </div>
                </div>
            </div>
        }
